{
  "name": "lab-wiki-backend",
  "version": "1.0.10",
  "description": "NTU NLP lab wiki backend",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node index.js start --start-nodemon-server",
    "start": "cross-env NODE_ENV=production node index.js start --start-nodemon-server",
    "setup": "node index.js setup",
    "data-init": "cross-env NODE_ENV=production node src/tools/data-parser.js ./data/",
    "data-init-dev": "cross-env NODE_ENV=development node src/tools/data-parser.js ./data/",
    "test": "cross-env NODE_ENV=test ./node_modules/mocha/bin/mocha ./src/test --timeout 15000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duckingod/lab-wiki-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/duckingod/lab-wiki-backend/issues"
  },
  "homepage": "https://github.com/duckingod/lab-wiki-backend#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "commander": "^2.17.1",
    "connect-history-api-fallback": "^1.5.0",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.5",
    "google-auth-library": "^0.10.0",
    "googleapis": "^21.3.0",
    "helmet": "^3.13.0",
    "jsonwebtoken": "^7.4.3",
    "mime": "^1.6.0",
    "moment": "^2.22.2",
    "nodemon": "^1.18.4",
    "quoted-printable": "^1.0.1",
    "request": "^2.88.0",
    "sequelize": "^4.38.0",
    "sqlite3": "^3.1.13",
    "utf8": "^2.1.2",
    "x-ray": "^2.3.3"
  },
  "standard": {
    "ignore": [
      "src/test/*",
      "src/static/*"
    ]
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^3.5.0",
    "standard": "^10.0.3",
    "supertest": "^3.1.0"
  }
}
